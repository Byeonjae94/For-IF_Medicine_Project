<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.example.user.medicine.Button_threeActivity">

    <EditText
        android:id="@+id/editText5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:ems="10"
        android:inputType="textPersonName"
        android:text="검색해주세요"
        android:layout_alignTop="@+id/imageButton5"
        android:layout_alignParentStart="true"
        android:layout_marginStart="47dp"
        android:layout_alignBottom="@+id/imageButton5" />

    <ImageButton
        android:id="@+id/imageButton5"
        android:layout_width="70dp"
        android:layout_height="70dp"
        android:scaleType="fitCenter"
        android:onClick="find_three"
        android:layout_marginTop="23dp"
        app:srcCompat="@drawable/photo1"
        android:layout_alignParentTop="true"
        android:layout_toEndOf="@+id/editText5"
        android:layout_marginStart="22dp" />

    <ListView
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_below="@id/editText5"
        android:id="@+id/listView"
        android:layout_margin="10dp"></ListView>

</RelativeLayout>
